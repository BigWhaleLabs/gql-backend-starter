{
  "name": "backend-starter",
  "version": "1.0.0",
  "description": "Backend starter template",
  "main": "dist/server.js",
  "repository": "https://github.com/Borodutch/backend-starter",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "prettier": "@big-whale-labs/prettier-config",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "add-aliases-and-run": "tsc-alias && node dist/server.js",
    "start": "yarn apply-migrations && node dist/server.js",
    "start-dev": "yarn prisma generate && NODE_ENV=development tsc-watch --skipLibCheck --onSuccess 'yarn add-aliases-and-run'",
    "build": "prisma generate && tsc --skipLibCheck && tsc-alias",
    "test": "yarn jest --forceExit --runInBand",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 ./src/",
    "lint-fix": "yarn eslint ./src/ --fix",
    "apply-migrations": "prisma migrate deploy"
  },
  "dependencies": {
    "@envelop/graphql-jit": "^8.0.3",
    "@envelop/live-query": "^7.0.0",
    "@graphql-tools/utils": "^10.5.5",
    "@graphql-yoga/plugin-defer-stream": "^3.7.0",
    "@graphql-yoga/plugin-graphql-sse": "^3.7.0",
    "@n1ru4l/graphql-live-query": "^0.10.0",
    "@n1ru4l/graphql-live-query-patch-jsondiffpatch": "^0.8.0",
    "@n1ru4l/in-memory-live-query-store": "^0.10.0",
    "@prisma/client": "^5.21.1",
    "@privy-io/server-auth": "^1.15.0",
    "@types/graphql-fields": "^1.3.9",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.21.1",
    "graphql": "^16.9.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.23.0",
    "graphql-yoga": "^5.7.0",
    "json-bigint-patch": "^0.0.8",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.21.1",
    "type-graphql": "^2.0.0-rc.2",
    "typegraphql-prisma": "^0.28.0"
  },
  "devDependencies": {
    "@big-whale-labs/bwl-eslint-backend": "^1.0.6",
    "@big-whale-labs/prettier-config": "^1.1.3",
    "@eslint/js": "^9.9.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@typegoose/typegoose": "^12.8.0",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9",
    "@types/node": "^22.8.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "http-graceful-shutdown": "^3.1.13",
    "module-alias": "^2.2.3",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.0",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.3.0"
  },
  "packageManager": "yarn@4.2.2"
}
