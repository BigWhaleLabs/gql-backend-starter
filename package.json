{
  "name": "backend-starter",
  "version": "1.0.0",
  "description": "Backend starter template",
  "main": "dist/server.js",
  "repository": "https://github.com/Borodutch/backend-starter",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "prettier": "@big-whale-labs/prettier-config",
  "license": "MIT",
  "scripts": {
    "distribute": "yarn build && node dist/server.js",
    "start": "tsc-watch --skipLibCheck --onSuccess 'node dist/server.js'",
    "build": "tsc --skipLibCheck",
    "test": "yarn jest --forceExit --runInBand",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 ./src/",
    "lint-fix": "yarn eslint ./src/ --fix"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@koa/cors": "^4.0.0",
    "amala": "^8.0.2",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "facebook-node-sdk": "^0.2.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.0",
    "koa-router": "^12.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.3"
  },
  "devDependencies": {
    "@big-whale-labs/bwl-eslint-backend": "^1.0.3",
    "@big-whale-labs/prettier-config": "^1.1.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typegoose/typegoose": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^4.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.10",
    "@types/supertest": "^2.0.12",
    "axios-mock-adapter": "^1.21.3",
    "eslint": "^8.36.0",
    "http-graceful-shutdown": "^3.1.13",
    "jest": "^29.5.0",
    "module-alias": "^2.2.2",
    "mongodb-memory-server": "^8.12.1",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.2"
  },
  "_moduleAliases": {
    "@": "dist"
